# AgentFlow Master 环境变量配置

# 服务器配置
AGENTFLOW_PORT=6767
AGENTFLOW_HOST=0.0.0.0

# 数据库配置
DATABASE_URL=sqlite://agentflow.db
AGENTFLOW_DB_PATH=agentflow.db

# 日志配置
RUST_LOG=info,agentflow_master=debug,agentflow_core=debug

# 任务执行配置
AGENTFLOW_MAX_CONCURRENT_TASKS=5
AGENTFLOW_DEFAULT_TASK_TIMEOUT=600

# 沙箱配置
AGENTFLOW_SANDBOX_ENABLED=true
AGENTFLOW_ALLOWED_DIRS=./

# 记忆配置
AGENTFLOW_MEMORY_ENABLED=true
AGENTFLOW_MEMORY_MAX_ENTRIES=10000
