# AgentFlow Master 配置示例
# 复制此文件为 .env 并根据需要修改

# 服务器配置
AGENTFLOW_SERVER_ADDR=0.0.0.0
AGENTFLOW_SERVER_PORT=6767

# 数据库配置
AGENTFLOW_DATABASE_URL=sqlite://agentflow.db

# Sandbox 配置
AGENTFLOW_SANDBOX_ENABLED=true
AGENTFLOW_SANDBOX_WORKSPACE=/tmp/agentflow/workspace
AGENTFLOW_SANDBOX_ALLOW_NETWORK=false
AGENTFLOW_SANDBOX_MAX_MEMORY=1G
AGENTFLOW_SANDBOX_MAX_CPU=2
AGENTFLOW_SANDBOX_TIMEOUT=300

# Memory 配置
AGENTFLOW_MEMORY_BACKEND=memory
AGENTFLOW_MEMORY_DATABASE_URL=sqlite://agentflow_memory.db
AGENTFLOW_MEMORY_DEFAULT_TTL=3600
AGENTFLOW_MEMORY_MAX_ENTRIES=10000
AGENTFLOW_MEMORY_PERSISTENCE=false

# Worker 配置
AGENTFLOW_WORKER_HEARTBEAT_TIMEOUT=60
AGENTFLOW_TASK_TIMEOUT=300
AGENTFLOW_MAX_CONCURRENT_TASKS=10

# 日志配置
AGENTFLOW_LOG_LEVEL=info
